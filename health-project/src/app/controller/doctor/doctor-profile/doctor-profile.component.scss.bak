#slider{width: 100%; max-height: 350px; background-color:#4a9bda;
    img{max-height: 250px;}
}
.d-block{ width:100%}
.dr-profile{width: 89%; margin:0px auto;
     position: relative; /*top: -165px;*/ top: 25px; display: flex; padding: 5px; margin-bottom: 20px;
    figure{ margin-bottom: 10px; width: 100%; max-width: 200px;
        padding: 15px; height: 220px;
        background-color: #fff;
        border: 1px solid rgb(43, 42, 42);
        img{width: 100%; height: 100%;}
    }
    .dr-details{ position: relative;  width: 100%; padding-left: 4%;color: #fff;
        h5{text-align: left; font-size: 28px; font-weight: 700;}
        .rating{ width: 100%; margin-bottom: 15px;
            span{font-size: 14px; margin-right: 10px; background-color: #257c2e; color: #fff; padding:2px 5px 4px;            
            i{font-size: 11px;}            
            }
            a{color: rgb(255, 255, 255); font-weight: 600;}
        }
        .fee{ margin-bottom: 15px; top: 10px; position: absolute; right: 10px;
            span{font-size: 14px; display: block; font-weight: 600; color: #ffffff;}            
        }
        p{font-size: 14px; margin: 0px;}        
    }
    .share{ margin-top: 30px; padding: 7px 0px;
        a{margin-right: 10px;}  
        :hover{color: #1055c4;}      
        span{float:right;
            :hover{color: #fff;}
        }
    }
    .video-consult{ width: 155px;}
    
}
.profile-details-box{ border:1px solid rgba(0, 0, 0, 0.125); margin-bottom: 10px;
    display: flex; padding: 15px;
    p {font-size: 14px; text-align: justify;}
}

// aside
.widget { position: relative; padding: 20px; font-size: 14px; overflow: hidden;
    border-radius: 10px; margin: 35px 0px; background-color: #def8f6;
    :after {
        content: '';
        position: absolute;
        top: 40px;
        bottom: 40px;
        left: 0;
        width: 3px;
        background-color: #21cdc0;
    }
    .widget-content{
        .widget-icon {
            color: #213360;
            font-size: 50px;
            margin-bottom: 6px;
            img{width: 45px;}
        }
        h4{font-size: 19px;margin-bottom: 23px;}
        ul{
            li{
                display: flex;
                justify-content: space-between;
                padding-bottom: 10px;
                margin-bottom: 10px;
                border-bottom: 1px solid #dcdee2;
                font-size: 14px;
            }
        }
    }
    .phone__number {
        font-size: 24px;
        margin-top: 5px;
        color: #fff!important;
        font-weight:600;    
        .fa-phone-volume {
            font-size: 17px;
            width: 40px;
            height: 40px;
            line-height: 40px;
            border-radius: 50%;
            text-align: center;
            margin-right: 10px;
            background-color: #3851a2;
        }
    }
}
.emergency-call-bg {background-color: #26a59a; color: #fff;}
// aside end

.treatments-Price{border: 1px solid rgba(0, 0, 0, 0.125); padding: 15px; margin: 30px 0px;
    h4{font-size: 20px;}
    ul{padding: 0px; list-style: none;
        li{padding: 8px 0px; width: 48%; border-bottom: 1px dotted #ccc;
            display: inline-block;margin: 0px 10px; font-size: 14px;
            span{float: right;}
        }
    }
}